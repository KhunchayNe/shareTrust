# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
NEXT_PUBLIC_SUPABASE_ACCESS_TOKEN=your_access_token_here

# LINE LIFF Configuration
NEXT_PUBLIC_LIFF_ID=your_liff_id_here
NEXT_PUBLIC_LINE_BOT_ID=your_bot_user_id

# Backend API
NEXT_PUBLIC_API_URL=https://your-nestjs-api.railway.app

# OAuth Providers (configured in Supabase dashboard)
NEXT_PUBLIC_ENABLE_GOOGLE_AUTH=true
NEXT_PUBLIC_ENABLE_FACEBOOK_AUTH=true
NEXT_PUBLIC_ENABLE_APPLE_AUTH=true
